{
	"https": {
		"port": 22222,
		"keyFile": "var/privkey.pem",
		"certFile": "var/fullchain.pem"
	},
	"express": {
		"trustProxy": false,
		"middlewares": {
			"rateLimit": {
				"enable": true,
				"windowMs": 3600000,
				"max": 1000,
				"standardHeaders": true
			},
			"json": {
				"enable": true,
				"limit": "10kb"
			},
			"helmet": {
			},
			"cors": {
				"enable": true,
				"origin": ["http://example.com"]
			},
			"static": {
				"enable": true,
				"directories": [
					{
						"directory": "test/static",
						"options": {
							"_comment": "supports express.static options except setHeaders"
						}
					}
				]
			}
		}
	},
	"winston": {
		"level": "debug",
		"errorLog": "var/logs/error.log",
		"exceptionLog": "var/logs/exception.log",
		"combinedLog": "var/logs/combined.log"
	},
	"application": {
		"yourKey": "yourValue"
	}
}
